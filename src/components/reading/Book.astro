---
import { Image } from 'astro:assets'
import s from './Reading.module.sass'

interface Props {
  image: string
  alt: string
}

const { image, alt } = Astro.props
---

<li class={s.list__item}>
  <a class={s.list__link} href="#link">
    <Image src={image} alt={alt} width={1600} height={2400} />
  </a>
</li>
